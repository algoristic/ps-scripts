<?xml version="1.0" encoding="UTF-8"?>
<types>
	<type>
		<name>Java</name>
		<file-extensions>
			<file-extension name="java">
				<comment-markers>
					<comment-marker>//</comment-marker>
					<comment-marker>/*</comment-marker>
					<comment-marker>*</comment-marker>
				</comment-markers>
			</file-extension>
		</file-extensions>		
	</type>
	<type>
		<name>C</name>
		<file-extensions>
			<file-extension name="c">
				<comment-markers>
					<comment-marker>//</comment-marker>
					<comment-marker>/*</comment-marker>
					<comment-marker>*</comment-marker>
				</comment-markers>
			</file-extension>
		</file-extensions>		
	</type>
	<type>
		<name>C++</name>
		<file-extensions>
			<file-extension name="cpp">
				<comment-markers>
					<comment-marker>//</comment-marker>
					<comment-marker>/*</comment-marker>
				</comment-markers>
			</file-extension>
		</file-extensions>		
	</type>
	<type>
		<name>Python</name>
		<file-extensions>
			<file-extension name="py">
			<comment-markers>
				<comment-marker>#</comment-marker>
			</comment-markers>
			</file-extension>
		</file-extensions>
	</type>
	<type>
		<name>Visual Basic .NET</name>
		<file-extensions>
			<file-extension name="vb">
